/*
  Icon Font: sitron_font
*/

@font-face {
  font-family: "sitron_font";
  src: url(<%=asset_path('sitron_font_c6f8d33bb292b56dc483d036427d8251.eot')%>);
  src: url(<%=asset_path('sitron_font_c6f8d33bb292b56dc483d036427d8251.eot?#iefix')%>) format("embedded-opentype"),
       url(<%=asset_path('sitron_font_c6f8d33bb292b56dc483d036427d8251.woff')%>) format("woff"),
       url(<%=asset_path('sitron_font_c6f8d33bb292b56dc483d036427d8251.ttf')%>) format("truetype"),
       url(<%=asset_path('sitron_font_c6f8d33bb292b56dc483d036427d8251.svg#sitron_font')%>) format("svg");
  font-weight: normal;
  font-style: normal;
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  @font-face {
    font-family: "sitron_font";
    src: url(<%=asset_path('sitron_font_c6f8d33bb292b56dc483d036427d8251.svg#sitron_font')%>) format("svg");
  }
}

[data-icon]:before { content: attr(data-icon); }

[data-icon]:before,
.fa-caminhao:before,
.fa-kombi:before,
.fa-motocicleta:before,
.fa-onibus:before,
.fa-passeio:before,
.fa-pickup:before,
.fa-sitron-marker:before,
.fa-sitron_marker:before,
.fa-van:before {
  display: inline-block;
  font-family: "sitron_font";
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  line-height: 1;
  text-decoration: inherit;
  text-rendering: optimizeLegibility;
  text-transform: none;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
  font-smoothing: antialiased;
}

.fa-caminhao:before { content: "\f10b"; }
.fa-kombi:before { content: "\f107"; }
.fa-motocicleta:before { content: "\f106"; }
.fa-onibus:before { content: "\f102"; }
.fa-passeio:before { content: "\f10c"; }
.fa-pickup:before { content: "\f108"; }
.fa-sitron-marker:before { content: "\f10a"; }
.fa-sitron_marker:before { content: "\f10e"; }
.fa-van:before { content: "\f109"; }
