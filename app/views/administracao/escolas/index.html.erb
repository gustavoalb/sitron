<table class='table'>
  <thead>
    <tr>
      <th>Municipio</th>
      <th>Dependencia administrativa</th>
      <th>Zona</th>
      <th>Codigo</th>
      <th>Nome</th>
      <th>Telefone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administracao_escolas.each do |administracao_escola| %>
      <tr>
        <td><%= administracao_escola.municipio %></td>
        <td><%= administracao_escola.dependencia_administrativa %></td>
        <td><%= administracao_escola.zona %></td>
        <td><%= administracao_escola.codigo %></td>
        <td><%= administracao_escola.nome %></td>
        <td><%= administracao_escola.telefone %></td>
        <td><%= link_to link_icone('detalhes'), administracao_escola,:class=>"btn btn-success btn-sm" %></td>
        <td><%= link_to link_icone('editar'), edit_administracao_escola_path(administracao_escola),class: "btn btn-warning btn-sm",data: { no_turbolink: true }  %></td>
        <td><%= link_to link_icone('apagar'), administracao_escola, method: :delete, data: { confirm: 'Are you sure?' },:class=>"btn btn-danger btn-sm" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>



<%= link_to link_icone('Cadastrar AdministracaoEscola','plus'), new_administracao_escola_path, :class=>"btn btn-large btn-success",data: { no_turbolink: true }  %>
